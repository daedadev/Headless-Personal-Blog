{
  "name": "headless-personal-blog",
  "version": "1.0.0",
  "description": "Backend to my personal blog",
  "main": "server.js",
  "scripts": {
    "begin": "npm run build && node server.js",
    "start": "node server.js",
    "start:original": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "seed": "node api/seeds/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daedadev/Shop-CMS.git"
  },
  "author": "Daniel Moore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daedadev/Shop-CMS/issues"
  },
  "homepage": "https://github.com/daedadev/Shop-CMS#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "connect-session-sequelize": "^7.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "sequelize": "^6.17.0"
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "jest-serial-runner": "^1.2.0",
    "jest-stare": "^2.3.0",
    "supertest": "^6.2.2",
    "tailwind-scrollbar": "^1.3.1"
  }
}
